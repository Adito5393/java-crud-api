{
	"categories":{
		"id":{"type":"integer","pk":true},
		"name":{"type":"string","length":255},
		"icon":{"type":"binary","nullable":true}
	},
	"comments":{
		"id":{"type":"integer","pk":true},
		"post_id":{"type":"integer","fk":"posts.id"},
		"message":{"type":"string","length":255}
	},
	"posts":{
		"id":{"type":"integer","pk":true},
		"user_id":{"type":"integer","fk":"users.id"},
		"category_id":{"type":"integer","fk":"categories.id"},
		"content":{"type":"string","length":255}
	},
	"post_tags":{
		"id":{"type":"integer","pk":true},
		"post_id":{"type":"integer","fk":"posts.id"},
		"tag_id":{"type":"integer","fk":"tags.id"}
	},
	"tags":{
		"id":{"type":"integer","pk":true},
		"name":{"type":"string","length":255}
	},
	"users":{
		"id":{"type":"integer","pk":true},
		"username":{"type":"string","length":255},
		"password":{"type":"string","length":255},
		"location":{"type":"geometry","nullable":true}
	},
	"countries":{
		"id":{"type":"integer","pk":true},
		"name":{"type":"string","length":255},
		"shape":{"type":"geometry"}
	},
	"events":{
		"id":{"type":"integer","pk":true},
		"name":{"type":"string","length":255},
		"datetime":{"type":"datetime"},
		"visitors":{"type":"integer"}
	},
	"products":{
		"id":{"type":"integer","pk":true},
		"name":{"type":"string","length":255},
		"price":{"type":"decimal","precision":10,"scale":2},
		"properties":{"type":"json"},
		"created_at":{"type":"datetime"},
		"deleted_at":{"type":"datetime","nullable":true}
	},
	"barcodes":{
		"id":{"type":"integer","pk":true},
		"product_id":{"type":"integer","fk":"products.id"},
		"hex":{"type":"string"},
		"bin":{"type":"binary"}
	}
}